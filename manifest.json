{
    "manifest_version": 3,
    "name": "Clear history and cache",
    "version": "1.0",
    "description": "A simple Chrome extension to clear browsing history, search history, and cache.",
    "permissions": ["history", "search", "browsingData"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }